<launch>
	<!-- Planner ID 			-->
	<!-- OMPL's PDST (1)		-->
	<!-- OMPL's RRT  (2)		-->
	<param name="planner" value="2" type="int"/>

	<!-- directory of alarm config files relative to the  -->
	<!-- sub8_trajectory_generator package root directory -->
	<param name="alarms_dir" value="cfg" type="string"/>
	
	<param name="sub_mass" value="25.0" type="double"/>

	<!-- OMPL constants -->
	<param name="state_validity_checking_resolution" value="0.03" type="double"/>
	<param name="propagation_step_size" value="0.1" type="double"/>
	<param name="min_control_duration" value="1.0" type="double"/>
	<param name="max_control_duration" value="100.0" type="double"/>
	
	<param name="position_gain" value="1.0" type="double"/>
	<param name="velocity_gain" value="1.0" type="double"/>
	<param name="angular_velocity_gain" value="1.0" type="double"/>
	<param name="orientation_gain" value="1.0" type="double"/>

	<!-- initial values for state space bounds -->
	<!-- position bounds, units = meters -->
	<param name="xmin" value="-1.0" type="double"/>
	<param name="xmax" value="1.0" type="double"/>
	<param name="ymin" value="-1.0" type="double"/>
	<param name="ymax" value="1.0" type="double"/>
	<param name="zmin" value="-1.0" type="double"/>
	<param name="zmax" value="1.0" type="double"/>

	<!-- velocity bounds, units = meters/sec -->
	<param name="v_xmin" value="-1.0" type="double"/>
	<param name="v_xmax" value="1.0" type="double"/>
	<param name="v_ymin" value="-1.0" type="double"/>
	<param name="v_ymax" value="1.0" type="double"/>
	<param name="v_zmin" value="-1.0" type="double"/>
	<param name="v_zmax" value="1.0" type="double"/>

	<!-- angular velocity bounds units = radians/sec -->
	<param name="w_xmin" value="-1.0" type="double"/>
	<param name="w_xmax" value="1.0" type="double"/>
	<param name="w_ymin" value="-1.0" type="double"/>
	<param name="w_ymax" value="1.0" type="double"/>
	<param name="w_zmin" value="-1.0" type="double"/>
	<param name="w_zmax" value="1.0" type="double"/>
</launch>