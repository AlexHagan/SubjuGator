<?xml version="1.0"?>
<!-- Sets robot_description paramater to Navigator's URDF -->
<launch>
  <param name="robot_description" textfile="$(find sub8_gazebo)/urdf/sub8.urdf"/>
  <rosparam param="robot_parameters">
    volume: 0.04822
    fluid_density: 999.1
    drag:
      linear_coeffs: [50, 50, 50]
      angular_coeffs: [10, 10, 10]
  </rosparam>
</launch>
